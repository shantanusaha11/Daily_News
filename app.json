{
  "expo": {
    "name": "Daily News",
    "slug": "Daily_News",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/newspaper_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/newspaper.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB562"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/newspaper_icon.png",
      "package": "com.dailynews.app"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
